{
  "name": "Schedulizer",
  "productName": "Schedulizer",
  "version": "1.0.2",
  "license": "MIT",
  "main": "./main.js",
  "repository": "https://github.com/peliukas/schedulizer-electron.git",
  "build": {
    "appId": "com.electron.schedulizer",
    "files": [
      "!publisher.json",
      "!README.md",
      "!updates.json"
    ],
    "win": {
      "target": "squirrel"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "build --win",
    "ship": "build --win -p always",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build && electron ./dist/index.html",
    "packagr": "ng-packagr -p ng-package.json",
    "postinstall": "install-app-deps",
    "dev": "NODE_ENV='development' npm run start",
    "dist": "build --x64 --ia32"
  },
  "private": false,
  "dependencies": {
